<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android"
    id="com.hacheiq.gpsetts"
    version="0.0.16">
    <name>GPSSettings</name>
    <description>The coolest Plugin ever implemented</description>
    <license>Apache 2.0</license>
    <keywords>cordova,coolest</keywords>
    <repo></repo>
    <issue></issue>
    <js-module src="www/GPSSettings.js" name="GPSSettings">
        <clobbers target="GPSSettings" />
    </js-module>
    <!-- android -->
    <platform name="android">
        <config-file target="res/xml/config.xml" parent="/*">
            <feature name="GPSSettings" >
                <param name="android-package" value="GPSSettings"/>
            </feature>
        </config-file>
        <source-file src="src/android/GPSSettings.java" target-dir="src/" />
    </platform>
</plugin>